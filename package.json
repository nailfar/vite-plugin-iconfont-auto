{
  "name": "vite-plugin-iconfont",
  "version": "1.0.7",
  "description": "This plugin is for auto download iconfont symbol js file",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "release": "npm build && release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nailfar/vite-plugin-iconfont-auto.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vite-plugin",
    "iconfont"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nailfar/vite-plugin-iconfont-auto/issues"
  },
  "homepage": "https://github.com/nailfar/vite-plugin-iconfont-auto#readme",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "release-it": "^15.6.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "rimraf": "^4.1.2"
  }
}
